wSS_top
{
    type            surfaceFieldValue;
    libs            ("libfieldFunctionObjects.so");

    log             false;
    writeControl    timeStep;
    writeInterval   1;
    writeFields     false;

    regionType      patch;
    name            topWall;

    operation       areaAverage;
    postOperation   mag;
    fields          (wallShearStress);
}

Urms_max
{
    type            volFieldValue;
    libs            ("libfieldFunctionObjects.so");

    log             false;
    writeControl    timeStep;
    writeInterval   1;
    writeFields     false;

    regionType      all;

    operation       max;
    postOperation   mag;
    fields          (UPrime2Mean);
}

