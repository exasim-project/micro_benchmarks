FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pisoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         780; //1722.8874;

deltaT          0.0078;

writeControl    runTime;

writeInterval   15.6; //17.2302;

purgeWrite      2;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    fieldAverage1
    {
        type            fieldAverage;
        libs            (fieldFunctionObjects);
        writeControl    writeTime;

        fields
        (
            U
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }

            p
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }
        );
    }
    processorField1
    {
        type            processorField;
        libs            (fieldFunctionObjects);
        writeControl    writeTime;
    }
    #include     "wallShearStress"
    #include     "solverInfo"
    #include     "fieldMinMax"
    #include     "outputDefs"
}


