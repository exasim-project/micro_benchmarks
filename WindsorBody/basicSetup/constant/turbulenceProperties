FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include       "${FOAM_CASE}/system/include/caseDefinition"

simulationType RAS;

LES
{
    LESModel        $LESturbModel;
    delta           $deltaTurb;

    turbulence      on;
    printCoeffs     on;
}

RAS
{
    RASModel        $RASturbModel;
    
    turbulence      on;
    printCoeffs     on;
}

