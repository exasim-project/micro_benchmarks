FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        DICGaussSeidel;
	tolerance       1e-03;
        relTol          0.01;
        nPreSweeps      0;
        nPostSweeps     2;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 1000;
        agglomerator    faceAreaPair;
        mergeLevels     1;
	maxIter 	1000;

    }

    pFinal
    {
        $p;
        tolerance       1e-04;
        relTol          0.001;
	maxIter 	1000;
    }

    "(U)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-08;
        relTol          0.1;
	maxIter 	1000;
    }

    "(U)Final"
    {
        $U;
        tolerance       1e-08;
        relTol          0;
	maxIter 	1000;
    }

    s
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-08;
        relTol          0;
        minIter	        1;
	maxIter 	1000;
    }
}

PIMPLE
{
    nOuterCorrectors 1;
    nCorrectors      3;
    nNonOrthogonalCorrectors 0;
    pRefCell         0;
    pRefValue        0;
}
