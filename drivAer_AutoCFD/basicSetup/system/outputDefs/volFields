#include        "../include/caseDefinition"

Uref            #eval{ sqrt(sqr($Uxref) + sqr($Uyref) + sqr($Uzref)) };

CourantNo
{
    type            CourantNo;
    libs            ( "libfieldFunctionObjects.so" );
    log             false;
    enabled         true;
    writeControl    writeTime;
}

