FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include       "include/caseDefinition"

numberOfSubdomains  16;

method              kahip; 

hierarchicalCoeffs
{
    n               (1 1 $numberOfSubdomains);
    delta           0.001;
    order           xyz;
}

kahipCoeffs
{
    config          fast; // fast | eco | strong
    imbalance       0.01;
}


