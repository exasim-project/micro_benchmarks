  "path": "postpro_hpc7a_opt_ompi5.json",
  "times_key": "clockTimeDelta",
  "unit": "s",
  "cpu_hr_ondemand": 0.0375,
  "cpu_hr_reserved": 0.017625,
  "variation_key": "preconditioner",
  "variation_map": {
    "FDIC": "hpc7a.96xl\n(PCG solver)",
    "none": "hpc7a.96xl\n(GAMG solver)"
  },
  "cpu_cores_per_node": 192
},
{
  "path": "postpro_c7i_opt.json",
  "times_key": "clockTimeDelta",
  "unit": "s",
  "cpu_hr_ondemand": 0.08925,
  "cpu_hr_reserved": 0.0341997,
  "variation_key": "preconditioner",
  "variation_map": {
    "FDIC": "c7i.48xl\n(PCG solver)",
    "none": "c7i.48xl\n(GAMG solver)"
