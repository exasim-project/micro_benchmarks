probes_azi_minus_056
{
    type            probes;
    libs            ( "libsampling.so" );
    writeControl    timeStep;
    writeInterval   1;
    fields          ( U );

    // Optional: do not recalculate cells if mesh moves
    fixedLocations  true;

    // Optional: interpolation scheme to use (default is cell)
    //interpolationScheme cellPatchConstrained;

    probeLocations
    (      
        (-0.30 0.56 0.00)
    );
}

